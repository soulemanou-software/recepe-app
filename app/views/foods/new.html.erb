<%= form_with model: @food, url: foods_path, local: true do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.label :measurement_unit %>
    <%= form.text_field :measurement_unit %>
    <%= form.label :price %>
    <%= form.number_field :price %>
    <%= form.label :quantity %>
    <%= form.number_field :quantity %>
    <%= form.submit %>
<% end %>